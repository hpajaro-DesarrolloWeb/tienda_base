<%= form_with(model: user, local: true) do |form| %>
  <% if user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :usuario %>
    <%= form.text_field :usuario, id: :user_usuario %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password, id: :user_password %>
  </div>

  <div class="field">
    <%= form.label :identidad %>
    <%= form.number_field :identidad, id: :user_identidad %>
  </div>

  <div class="field">
    <%= form.label :idtipoentidad %>
    <%= form.number_field :idtipoentidad, id: :user_idtipoentidad %>
  </div>

  <div class="field">
    <%= form.label :idestadousuario %>
    <%= form.number_field :idestadousuario, id: :user_idestadousuario %>
  </div>

  <div class="field">
    <%= form.label :idrol %>
    <%= form.number_field :idrol, id: :user_idrol %>
  </div>

  <div class="field">
    <%= form.label :eliminado %>
    <%= form.check_box :eliminado, id: :user_eliminado %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
