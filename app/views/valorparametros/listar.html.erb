
<h4>Valores del Parametro</h4>
<a href="/valp/<%= @idvp %>/new" class="btn btn-primary btn-sm">Nuevo</a>
 
 <p id="notice"><%= notice %></p>
<table class="table-striped">
  <thead>
    <tr>
      <th>Id</th>
      <th>Valor</th>
      <th>Estado</th>
      <th>Orden</th>
      <th>Eliminado</th>
      <th>Idpadre</th>
      <th> </th>
    </tr>
  </thead>

  <tbody>
    <% @valorparametros.each do |valorparametro| %>
      <tr>
        <td><a class="llaves" href="/valorparametros/<%= valorparametro.id %>"><%= valorparametro.id %></a></td>
        <td><%= valorparametro.valor %></td>
        <td><%= valorparametro.estadovalorparametro %></td>
        <td><%= valorparametro.orden %></td>
        <td><%= valorparametro.eliminado %></td>
        <td><%= valorparametro.idpadre %></td>
        <td><%= link_to 'Destroy', valorparametro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<div class="digg_pagination"> 
  <%= will_paginate @valorparametros, :container => false %>
</div>

